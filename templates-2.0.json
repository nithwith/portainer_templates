{
	"version": "2",
	"templates": [
		{
			"type": 3,
			"title": "Odoo",
			"description": "Odoo instance with postgres ",
			"note": "Deploys your Odoo instance with postgres.",
			"categories": [
				"ERP"
			],
			"platform": "linux",
			"logo": "https://odoocdn.com/openerp_website/static/src/img/assets/png/odoo_logo.png",
			"repository": {
				"url": "https://github.com/nithwith/templates",
				"stackfile": "stacks/odoo/docker-compose.yml"
			},
			"env": [{
				"name": "ODOO_VERSION",
				"label": "Odoo version",
				"description": "12.0"
			},
			{
				"name": "PROJECT_NAME",
				"label": "Project name",
				"description": "Odoo"
			},
			{
				"name": "PROJECT_URL",
				"label": "Project url",
				"description": "odoo.yourdomain.com"
			},
			{
				"name": "HTTP_PORT",
				"label": "Http port",
				"description": "8069"
			},
			{
				"name": "POLL_PORT",
				"label": "Poll port",
				"description": "8072"
			},
			{
				"name": "DB_PORT",
				"label": "database port",
				"description": "5432"
			}]
		},
		{
			"type": 3,
			"title": "Pgadmin",
			"description": "Pgadmin instance",
			"note": "Deploys your Pgadmin instance.",
			"categories": [
				"Monitoring"
			],
			"platform": "linux",
			"logo": "https://www.todopostgresql.com/wp-content/uploads/2018/08/cursoPgAdmin4.png",
			"repository": {
				"url": "https://github.com/nithwith/templates",
				"stackfile": "stacks/pgadmin/docker-compose.yml"
			},
			"env": [{
				"name": "PGADMIN_USER",
				"label": "Pgadmin user",
				"description": "admin"
			},
			{
				"name": "PGADMIN_PASSWORD",
				"label": "Pgadmin password",
				"description": "admin"
			},
			{
				"name": "PGADMIN_URL",
				"label": "Pgadmin url",
				"description": "pgadmin.yourdomain.com"
			}]
		}
	]
}